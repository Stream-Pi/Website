{
  "name": "streampi-website",
  "version": "2.0.0",
  "author": "Samuel Qui√±ones",
  "description": "The official Stream-Pi website",
  "repository": {
    "type": "git",
    "url": "https://github.com/stream-pi/website"
  },
  "bugs": {
    "url": "https://github.com/stream-pi/website/issues"
  },
  "config": {
    "static_out": "out"
  },
  "scripts": {
    "dev": "npm run clean && next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "npm run clean && next build",
    "start": "next start -p 8080",
    "indev": "next start -p 3012",
    "static": "cross-env-shell \"npm run build && next export -o $npm_package_config_static_out\"",
    "serve-static": "cross-env-shell \"serve $npm_package_config_static_out\"",
    "clean": "cross-env-shell \"rimraf .next && rimraf $npm_package_config_static_out\"",
    "clean:light": "cross-env-shell \"rimraf .next/* && rimraf $npm_package_config_static_out/*\"",
    "preinstall": "node ./.scripts/nodeVersionCheck.js && node ./.scripts/npmVersionCheck.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "animate.css": "^4.1.1",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "formik": "^2.2.6",
    "gray-matter": "^4.0.2",
    "next": "10.0.6",
    "node-sass": "^5.0.0",
    "nodemailer": "^6.4.18",
    "react": "17.0.1",
    "react-bootstrap": "^1.5.0",
    "react-dom": "17.0.1",
    "react-google-recaptcha": "^2.1.0",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/node": "^14.14.28",
    "@types/nodemailer": "^6.4.0",
    "@types/react": "^17.0.2",
    "@types/react-google-recaptcha": "^2.1.0",
    "cross-env": "^7.0.3",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "typescript": "^4.2.2"
  }
}
